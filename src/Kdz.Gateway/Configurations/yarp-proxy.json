{
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "/{**catch-all}"
        },
        "AuthorizationPolicy": "accessUrlPolicy",
        "Transforms": [
          {
            "RequestHeader": "Authorization",
            "Set": "Basic Y2FyZGlvOnZpcnR1YWw="
          }
        ]
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost/"
          }
        }
      }
    }
  }
}